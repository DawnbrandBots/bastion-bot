{
	"name": "bastion-bot",
	"version": "0.0.1",
	"main": "dist/index.js",
	"repository": "https://github.com/DawnbrandBots/bastion-bot.git",
	"author": "Kevin Lu",
	"license": "AGPL-3.0-or-later",
	"private": true,
	"scripts": {
		"start": "node --enable-source-maps -r dotenv/config -r reflect-metadata dist",
		"register": "node --enable-source-maps -r dotenv/config -r reflect-metadata dist --deploy-slash",
		"build": "tsc",
		"clean": "tsc --build --clean",
		"test": "jest",
		"lint": "eslint src test"
	},
	"engines": {
		"node": ">=16.6",
		"yarn": "1.22.x"
	},
	"dependencies": {
		"debug": "^4.3.2",
		"discord.js": "^13.1.0",
		"dotenv": "^10.0.0",
		"node-fetch": "^2.6.1",
		"reflect-metadata": "^0.1.13",
		"supports-color": "^9.0.2",
		"tsyringe": "^4.6.0"
	},
	"devDependencies": {
		"@alphakretin/essentials": "^1.0.0",
		"@types/debug": "^4.1.7",
		"@types/jest": "^27.0.1",
		"@types/node": "^16.7.2",
		"@types/node-fetch": "^2.5.12",
		"jest": "^27.0.6",
		"ts-jest": "^27.0.5"
	},
	"jest": {
		"clearMocks": true,
		"collectCoverage": true,
		"coverageDirectory": "coverage",
		"coverageProvider": "v8",
		"preset": "ts-jest",
		"setupFiles": [
			"reflect-metadata"
		]
	},
	"eslintConfig": {
		"extends": "@alphakretin"
	},
	"prettier": "@alphakretin/essentials"
}
