{
  "name": "discord-bot-template",
  "version": "0.0.1",
  "main": "dist/index.js",
  "repository": "https://github.com/kevinlul/discord-bot-template.git",
  "author": "Kevin Lu",
  "license": "AGPL-3.0-or-later",
  "private": true,
  "scripts": {
    "start": "node --enable-source-maps --unhandled-rejections=strict -r dotenv/config -r reflect-metadata dist",
    "register": "node --enable-source-maps --unhandled-rejections=strict -r dotenv/config -r reflect-metadata dist --deploy-slash",
    "build": "tsc",
    "clean": "tsc --build --clean",
    "test": "jest",
    "lint": "eslint src test"
  },
  "engines": {
    "node": ">=14.x",
    "yarn": "1.x"
  },
  "dependencies": {
    "debug": "^4.3.2",
    "discord.js": "^13.0.0-dev.t1627831263.774f5b7",
    "dotenv": "^10.0.0",
    "node-fetch": "^2.6.1",
    "reflect-metadata": "^0.1.13",
    "supports-color": "^9.0.2",
    "tsyringe": "^4.6.0"
  },
  "devDependencies": {
    "@alphakretin/essentials": "^1.0.0",
    "@types/debug": "^4.1.7",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.17.7",
    "@types/node-fetch": "^2.5.12",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.4"
  },
  "jest": {
    "clearMocks": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageProvider": "v8",
    "preset": "ts-jest",
    "setupFiles": [
      "reflect-metadata"
    ]
  },
  "eslintConfig": {
    "extends": "@alphakretin"
  },
  "prettier": "@alphakretin/essentials"
}
